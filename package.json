    "hexo-generator-tag": "^0.2.0",
    "hexo-renderer-ejs": "^0.2.0",
    "hexo-renderer-stylus": "^0.3.1",
    "hexo-renderer-marked": "^0.2.10",
    "hexo-server": "^0.2.0"
  },
  "devDependencies": {
    "budo": "^8.2.2",
    "concurrently": "^2.0.0",
    "surge": "^0.17.7"
  }
}
